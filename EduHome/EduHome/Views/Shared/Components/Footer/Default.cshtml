@model Bios

<!-- Footer Start -->
<footer class="footer-area">
    <div class="main-footer">
        <div class="container">
            <div class="row">
                <div class="col-md-4 col-sm-6 col-xs-12">
                    <div class="single-widget pr-60">
                        <div class="footer-logo pb-25">
                            <a asp-controller="Home" asp-action="Index"><img src="~/img/logo/@Model.Logo" alt="eduhome"></a>
                        </div>
                        <p>@Model.Description</p>
                        <div class="footer-social">
                            <ul>
                                <li><a href="@Model.FacebookUrl"><i class="zmdi zmdi-facebook"></i></a></li>
                                <li><a href="@Model.PinterestUrl"><i class="zmdi zmdi-pinterest"></i></a></li>
                                <li><a href="@Model.VcontactUrl"><i class="zmdi zmdi-vimeo"></i></a></li>
                                <li><a href="@Model.TwitterUrl"><i class="zmdi zmdi-twitter"></i></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="col-md-3 col-sm-6 col-xs-12">
                    <div class="single-widget">
                        <h3>information</h3>
                        <ul>
                            <li><a href="#">addmission</a></li>
                            <li><a href="#">Academic Calender</a></li>
                            <li><a asp-controller="Event" asp-action="Index">Event List</a></li>
                            <li><a href="#">Hostel &amp; Dinning</a></li>
                            <li><a href="#">TimeTable</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-md-2 col-sm-6 col-xs-12">
                    <div class="single-widget">
                        <h3>useful links</h3>
                        <ul>
                            <li><a asp-controller="Course" asp-action="Index">our courses</a></li>
                            <li><a asp-controller="About" asp-action="Index">about us</a></li>
                            <li><a asp-controller="Teacher" asp-action="Index">teachers &amp; faculty</a></li>
                            <li><a href="#">teams &amp; conditions</a></li>
                            <li><a asp-controller="Event" asp-action="Index">our events</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-md-3 col-sm-6 col-xs-12">
                    <div class="single-widget">
                        <h3>get in touch</h3>
                        <p>@Html.Raw(Model.Adress)</p>
                        <p>@Model.FooterNumber1<br>@Model.FooterNumber2</p>
                        <p>@Model.infoEmail<br>@Model.infoSite</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="footer-bottom text-center">
        <div class="container">
            <div class="row">
                <div class="col-xs-12">
                    <p>Copyright © <span>HasTech</span> 2017. All Right Reserved By Hastech.</p>
                </div>
            </div>
        </div>
    </div>
</footer>
<!-- Footer End -->